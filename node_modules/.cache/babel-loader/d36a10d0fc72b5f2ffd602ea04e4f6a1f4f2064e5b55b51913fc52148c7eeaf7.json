    };\n  },\n  methods: {\n    async signup() {\n      const auth = getAuth();\n      try {\n        // Cria o usuário no Firebase Authentication\n        const userCredential = await createUserWithEmailAndPassword(\n          auth,\n          this.email,\n          this.password\n        );\n\n        // Atualiza o perfil do usuário com o nome\n        await updateProfile(userCredential.user, { displayName: this.name });\n\n        console.log(\"Usuário criado com sucesso\");\n        // Redirecione para a página após o cadastro\n        this.$router.push(\"/dashboard\");\n      } catch (error) {\n        console.error(\"Erro ao criar usuário:\", error.message);\n      }\n    },\n  },\n};\n</script>\n"],"mappings":";AAiBA,SACEA,OAAO,EACPC,8BAA8B,EAC9BC,aAAa,QACR,eAAe;AAEtB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,MAAMC,IAAG,GAAIT,OAAO,CAAC,CAAC;MACtB,IAAI;QACF;QACA,MAAMU,cAAa,GAAI,MAAMT,8BAA8B,CACzDQ,IAAI,EACJ,IAAI,CAACJ,KAAK,EACV,IAAI,CAACC,QACP,CAAC;;QAED;QACA,MAAMJ,aAAa,CAACQ,cAAc,CAACC,IAAI,EAAE;UAAEC,WAAW,EAAE,IAAI,CAACR;QAAK,CAAC,CAAC;QAEpES,OAAO,CAACC,GAAG,CAAC,4BAA4B,CAAC;QACzC;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;MACjC,EAAE,OAAOC,KAAK,EAAE;QACdJ,OAAO,CAACI,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAACC,OAAO,CAAC;MACxD;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}